{
    "playerOne": {
        "type": "Player",
        "cameraFocus": true,
        "keyBindings": {
            "forward": "KeyW",
            "backward": "KeyS",
            "strafe-right": "KeyD",
            "strafe-left": "KeyA",
            "rotate-cw": ["KeyL", "KeyE"],
            "rotate-cc": ["KeyJ", "KeyQ"],
            "dash-forward": "KeyI",
            "dash-backward": "KeyK",
            "fire": "Space",
            "puke": "KeyP"
        },
        "mouseBindings": {
            "mouse-rotation": "mousemove",
            "mouse-dash-forward": "0",
            "mouse-dash-backward": "2"
        },
        "physics": {
            "force": 2802,
            "angularForce": 10,
            "turnAmount": "{{4*Math.PI}}",
            "dampeningForceScaler": 0.1,
            "boostMultipier": 2.5,
            "strafeMultipier": 1,
            "shape": {
                "width": 2,
                "height": 1,
                "position": {"x": 0, "y": 0},
                "angle": 0
            },
            "fixture": {
                "density": 1,
                "friction": 0.5,
                "restitution": 0.5
            },
            "body":{
                "isDynamic": true,
                "position": {"x": 0, "y": 0},
                "angle": 0,
                "linearDamping": "{{playerOne.physics.force/800}}",
                "angularDamping": 0,
                "fixedRotation": false
            }
        },
        "mesh": {
            "width": "{{playerOne.physics.shape.width}}",
            "height": "{{playerOne.physics.shape.height}}",
            "depth": 1,
            "position": "{{playerOne.physics.body.position}}",
            "castShadow": true,
            "receiveShadow": true,
            "material": {
                "materialType": "MeshStandardMaterial",
                "loaderType": "TextureLoader",
                "texturePath": "textures/spaceships/arrow_thing.png",
                "roughness": 0.8
            }
        }
    },

    "box1": {
        "type": "Player",
        "physics": {
            "shape": {
                "width": 4,
                "height": 1,
                "position": {"x": 0, "y": 5},
                "angle": 0
            },
            "fixture": {
                "density": 1,
                "friction": 0.5,
                "restitution": 0.5
            },
            "body":{
                "isDynamic": true,
                "position": {"x": 0, "y": 0},
                "angle": 0,
                "linearDamping": 0,
                "angularDamping": 0,
                "fixedRotation": false
            }
        },
        "mesh": {
            "width": "{{box1.physics.shape.width}}",
            "height": "{{box1.physics.shape.height}}",
            "depth": 4,
            "position": "{{box1.physics.body.position}}",
            "castShadow": true,
            "receiveShadow": true,
            "material": {
                "materialType": "MeshStandardMaterial",
                "loaderType": "TextureLoader",
                "texturePath": "textures/spaceships/arrow_thing.png",
                "roughness": 0.8
            }
        }
    },

    "SoccerBall": {
        "type": "SoccerBall",
        "physics": {
            "shape": {
                "radius": 4
            },
            "fixture": {
                "density": 0.1,
                "friction": 0.5,
                "restitution": 0.8
            },
            "body":{
                "isDynamic": true,
                "position": {"x": 0, "y": -4},
                "angle": 0
            }
        },
        "mesh": {
            "radius": "{{SoccerBall.physics.shape.radius}}",
            "detail": 2,
            "position": "{{SoccerBall.physics.body.position}}",
            "castShadow": true,
            "receiveShadow": true,
            "material": {
                "materialType": "MeshStandardMaterial",
                "loaderType": "TextureLoader",
                "texturePath": "textures/checker/redwhite.jpg",
                "roughness": 0.8
            }
        }
    }
}
